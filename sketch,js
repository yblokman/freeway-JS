
function setup() {
  createCanvas(500, 400);
  somTrilha.loop()
}

function draw() {
  background(imagemDaEstrada);
  movePlayer();
  mostraPlayer();
  moveCarro();
  mostraCarro();
  voltaPosicao();
  colidiu();
  incluiPontos();
  marcaPonto();
}                                                                     
